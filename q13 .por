programa {
    inclua biblioteca Matematica --> mat
	funcao inicio() {
	    
	
		real A, B, C
		
		escreva("Digite o valor de A: ")
		leia(A)
		escreva("Digite o valor de B: ")
		leia(B)
		escreva("Digite o valor de C: ")
		leia(C)
		
		se(A+B > C){
		    escreva("O valor de A + B e maior que C")
		    
		}senao{
		    escreva("O valor de A + B e não maior que C")
		}
		
	}
}
